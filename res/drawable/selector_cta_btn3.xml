<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="3.0dip" android:color="@color/button_cta_inactive" />
            <corners android:radius="25.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="3.0dip" android:color="@color/button_cta_inactive" />
            <corners android:radius="25.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="3.0dip" android:color="@color/royal_blue" />
            <corners android:radius="25.0dip" />
        </shape>
    </item>
</selector>