<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/button_cta_inactive" />
            <corners android:radius="@dimen/btn_cta_new_radius" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/font_AFCBF6" />
            <corners android:radius="@dimen/btn_cta_new_radius" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/royal_blue" />
            <corners android:radius="@dimen/btn_cta_new_radius" />
        </shape>
    </item>
</selector>